                </button>
            </div>
            <div class="modal-body py-10 px-lg-17">
                <div class="row mb-5">
															<!--begin::Col-->
															<div class="col-md-6 fv-row">
																<!--begin::Label-->
																<label class="required fs-5 fw-bold mb-2">Shop Name</label>
																<!--end::Label-->
																<!--begin::Input-->
																<input type="text" id="txtName" class="form-control form-control-solid" placeholder="" />
																<!--end::Input-->
															</div>
															<!--end::Col-->
															<!--begin::Col-->
															<div class="col-md-6 fv-row">
																<!--end::Label-->
																<label class="required fs-5 fw-bold mb-2">Shop Code</label>
																<!--end::Label-->
																<!--end::Input-->
																<input type="text" id="txtCode" class="form-control form-control-solid" placeholder="" />
																<!--end::Input-->
															</div>
															<!--end::Col-->
														</div>
<!--begin::Input group-->
														<div class="row mb-5">
															<!--begin::Col-->
															<div class="col-md-12 fv-row">
																<!--begin::Label-->
																<label class="required fs-5 fw-bold mb-2">Price</label>
																<!--end::Label-->
																<!--begin::Input-->
																<input type="number" id="txtPrice" class="form-control form-control-solid" placeholder="" />
																<!--end::Input-->
															</div>
															<!--end::Col-->
															
														</div>
														<!--end::Input group-->
                <div class="d-flex flex-column mb-5 fv-row">
															<!--begin::Label-->
															<label class="required fs-5 fw-bold mb-2">Hash Rate</label>
															<!--end::Label-->
															<!--begin::Input-->
															<input class="form-control form-control-solid" id="txtHashRate" placeholder="" name="txtContractAddress" />
															<!--end::Input-->
														</div>
                                                        <div class="d-flex flex-column mb-5 fv-row">
															<!--begin::Label-->
															<label class="required fs-5 fw-bold mb-2">Shop Image Url</label>
															<!--end::Label-->
															<!--begin::Input-->
															<input class="form-control form-control-solid" id="txtImageUrl" placeholder="" name="txtTokenImageUrl" />
															<!--end::Input-->
														</div>

                                                        <div class="row mb-5">
															<!--begin::Col-->
															<div class="col-md-6 fv-row">
																<!--begin::Label-->
																<label class="required fs-5 fw-bold mb-2">Time To Use</label>
																<!--end::Label-->
																<!--begin::Input-->
																<input type="number" id="txtTimeToUse" class="form-control form-control-solid" placeholder="" />
																<!--end::Input-->
															</div>
															<!--end::Col-->
															
														</div>

                
            </div>
            <div class="modal-footer">

<!-- Modal Deposit Section Start-->
<div class="modal fade" id="modal_deposit" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered">
            <div class="modal-content">
                <div class="modal-header">

                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"><i class="icon-x"></i></button>
                </div>
                <div class="modal-body">
                    <form>
                        <h2>Deposit <span class="tokenCode"></span></h2>
                        <h3>This address support <span class="tokenCode"></span> Bsc BEP20</h3>
                        <div class="FullNameFild">
                            <lable>Qr Code Address</lable>
                            <div style="text-align: center; width: 100%; background: white; padding: 20px 0px;"
                                 class="qr-code" id="qrcodePublishKey">
                            </div>
                        </div>
                        <div class="PasswordFild mt-3">
                            <lable>Wallet Address</lable>
                            <input type="text" id="txtPublishKey" readonly>
                        </div>
                        <div class="mb-3 text-center">
                            <label>
                                Minimum deposit <span class="minDeposit"></span> <span class="tokenCode"></span>
                            </label>
                        </div>
                        <div class="mb-4">
                            <button id="btnCopyPublishKey">Copy address</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
<!-- Modal Deposit Section End-->

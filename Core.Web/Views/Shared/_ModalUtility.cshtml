<!-- Modal Transfer Section Start-->
<div class="modal fade" id="modal_transfer" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">

                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"><i class="icon-x"></i></button>
            </div>
            <div class="modal-body">
                <form>
                    @Html.AntiForgeryToken()
                    <h2>Transfer <span class="tokenCode"></span></h2>
                    <h3>This address support <span class="tokenCode"></span> BSC BEP20</h3>
                    <div>
                        <lable>Wallet <span class="tokenCode"></span></lable>
                        <input type="text" class="balance" readonly>
                    </div>
                    <div>
                        <lable>Amount</lable>
                        <input type="text" id="txtTransferAmount" value="0.00" readonly class="numberFormat">
                        <span class="lblErrorInsufficient  text-danger"></span>
                    </div>
                    <div>
                        <lable>Fee</lable>
                        <input type="text" id="txtTransferFeeAmount" value="0.00" readonly>
                    </div>
                    <div>
                        <lable>Amount Receive</lable>
                        <input type="text" id="txtTransferAmountReceive" value="0.00" readonly>
                    </div>
                    <div>
                        <lable>Receive Address</lable>
                        <input type="text" id="txtTransferReceiveAddress" readonly>
                    </div>
                    <div class="mb-3 text-center">
                        <label>
                            Minimum transfer <span class="minTransfer"></span> <span class="tokenCode"></span>
                        </label>
                    </div>
                    <div class="mb-4">
                        <button id="btnConfirmTransfer">Transfer</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
